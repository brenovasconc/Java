import java.util.Scanner;
public class Questao13 {
public static void main (String[] args){
Scanner input = new Scanner (System.in);    
int n,i = 2,fibonacci,a=0,b=1;
System.out.printf("Digite a quantidade de termos que vocÃª deseja:\n");
    n = input.nextInt();
if( n == 1) 
    System.out.printf("%d",a);
else if( n == 2)
    System.out.printf("%d\n%d",a,b);
else{
    System.out.printf("%d\n%d\n", a,b);
for(i=2;i < n;i++){
 fibonacci = a + b;
System.out.printf("%d\n", fibonacci);
a = b;
b = fibonacci;
}
}
}
