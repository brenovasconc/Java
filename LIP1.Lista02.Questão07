package LIP1.Lista02;

/**
 *
 * @author gean_
 */
import java.util.Scanner;
public class QuestaoFibonacci {
public static void Fibonacci(int x){
int i,a=0,b=1,aux;
for(i=2;i<x;i++){
    aux=a+b;
    a=b;
    b=aux;
System.out.printf("%d\n",aux);}
}

public static void main(String[] args){
Scanner input = new Scanner(System.in);
int x;
System.out.printf("Digite a quantidade que deseja:\n");
    x = input.nextInt();

if(x==1)
    System.out.printf("0\n");
else if(x==2)
    System.out.printf("0\n1\n");
else{
    System.out.printf("0\n1\n");
    Fibonacci(x);
}
    
}
}
