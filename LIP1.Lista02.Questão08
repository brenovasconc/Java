package LIP1.Lista02;

/**
 *
 * @author Breno Vasconcelos
 */
public class Questão01 {
    public static void PrimeiroMetodo() {
      System.out.println("Inicio da execução o primeiro método!");
      SegundoMetodo();
      System.out.println("Fim da execução do primeiro método!");
    }
    public static void SegundoMetodo() {
      System.out.println("Inicio da execução o segundo método!");
      double v[] = new double[5];
      try{
      for (int i = 0; i <= 10; i++) {
        v[i] = i;
        System.out.println(i);
      }
      }
      catch(ArrayIndexOutOfBoundsException x){
    System.err.print(x.getCause());
    System.err.print(x.getMessage());}
      finally{
      System.out.println("Fim da execução do segundo método!");
    }
    }
    public static void main(String[] args) {
      System.out.println("Inicio da main");
      PrimeiroMetodo();
      System.out.println("Fim da main");
    }
}   

